{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Rivulet": {
    "MaxDegreeOfParallelism": 10,
    "MaxRetries": 3,
    "BaseDelay": "00:00:00.5",
    "MaxDelay": "00:00:05",
    "BufferSize": 100,
    "ErrorMode": "CollectAndContinue",
    "OrderedOutput": false
  }
}
