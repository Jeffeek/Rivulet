	<!-- FILE LAYOUT PATTERNS -->
	<!-- ═══════════════════════════════════════════════════════════════════════════ -->

	

	<!-- ─────────────────────────────────────────────────────────────────────────── -->
	<!-- MEMBER ORDERING IN CLASSES -->
	<!-- ─────────────────────────────────────────────────────────────────────────── -->
	<!-- Order: Constants → Static readonly → Fields → Constructors → Properties → Methods -->
	<s:String x:Key="/Default/CodeStyle/CodeFormatting/CSharpCodeStyle/MEMBER_ARRANGEMENT_PATTERNS/@EntryValue">&lt;?xml version="1.0" encoding="utf-8"?&gt;
&lt;Patterns xmlns="urn:schemas-jetbrains-com:member-reordering-patterns"&gt;
  &lt;TypePattern DisplayName="Default Pattern" Priority="1000"&gt;
    &lt;TypePattern.Match&gt;
      &lt;Or&gt;
        &lt;And&gt;
          &lt;Kind Is="class" /&gt;
        &lt;/And&gt;
        &lt;And&gt;
          &lt;Kind Is="struct" /&gt;
        &lt;/And&gt;
        &lt;And&gt;
          &lt;Kind Is="interface" /&gt;
        &lt;/And&gt;
      &lt;/Or&gt;
    &lt;/TypePattern.Match&gt;
    &lt;Entry DisplayName="Constants"&gt;
      &lt;Entry.Match&gt;
        &lt;And&gt;
          &lt;Kind Is="constant" /&gt;
        &lt;/And&gt;
      &lt;/Entry.Match&gt;
      &lt;Entry.SortBy&gt;
        &lt;Access Order="public internal protected-internal protected private" /&gt;
        &lt;Name /&gt;
      &lt;/Entry.SortBy&gt;
    &lt;/Entry&gt;
    &lt;Entry DisplayName="Static readonly fields"&gt;
      &lt;Entry.Match&gt;
        &lt;And&gt;
          &lt;Kind Is="field" /&gt;
          &lt;Static /&gt;
          &lt;Readonly /&gt;
        &lt;/And&gt;
      &lt;/Entry.Match&gt;
      &lt;Entry.SortBy&gt;
        &lt;Access Order="public internal protected-internal protected private" /&gt;
        &lt;Name /&gt;
      &lt;/Entry.SortBy&gt;
    &lt;/Entry&gt;
    &lt;Entry DisplayName="Static fields"&gt;
      &lt;Entry.Match&gt;
        &lt;And&gt;
          &lt;Kind Is="field" /&gt;
          &lt;Static /&gt;
        &lt;/And&gt;
      &lt;/Entry.Match&gt;
      &lt;Entry.SortBy&gt;
        &lt;Access Order="public internal protected-internal protected private" /&gt;
        &lt;Name /&gt;
      &lt;/Entry.SortBy&gt;
    &lt;/Entry&gt;
    &lt;Entry DisplayName="Fields"&gt;
      &lt;Entry.Match&gt;
        &lt;And&gt;
          &lt;Kind Is="field" /&gt;
        &lt;/And&gt;
      &lt;/Entry.Match&gt;
      &lt;Entry.SortBy&gt;
        &lt;Readonly /&gt;
        &lt;Access Order="public internal protected-internal protected private" /&gt;
        &lt;Name /&gt;
      &lt;/Entry.SortBy&gt;
    &lt;/Entry&gt;
    &lt;Entry DisplayName="Constructors"&gt;
      &lt;Entry.Match&gt;
        &lt;And&gt;
          &lt;Kind Is="constructor" /&gt;
        &lt;/And&gt;
      &lt;/Entry.Match&gt;
      &lt;Entry.SortBy&gt;
        &lt;Static /&gt;
        &lt;Access Order="public internal protected-internal protected private" /&gt;
      &lt;/Entry.SortBy&gt;
    &lt;/Entry&gt;
    &lt;Entry DisplayName="Properties"&gt;
      &lt;Entry.Match&gt;
        &lt;And&gt;
          &lt;Kind Is="property" /&gt;
        &lt;/And&gt;
      &lt;/Entry.Match&gt;
      &lt;Entry.SortBy&gt;
        &lt;Static /&gt;
        &lt;Access Order="public internal protected-internal protected private" /&gt;
        &lt;Name /&gt;
      &lt;/Entry.SortBy&gt;
    &lt;/Entry&gt;
    &lt;Entry DisplayName="Events"&gt;
      &lt;Entry.Match&gt;
        &lt;And&gt;
          &lt;Kind Is="event" /&gt;
        &lt;/And&gt;
      &lt;/Entry.Match&gt;
      &lt;Entry.SortBy&gt;
        &lt;Static /&gt;
        &lt;Access Order="public internal protected-internal protected private" /&gt;
        &lt;Name /&gt;
      &lt;/Entry.SortBy&gt;
    &lt;/Entry&gt;
    &lt;Entry DisplayName="Methods"&gt;
      &lt;Entry.Match&gt;
        &lt;And&gt;
          &lt;Kind Is="method" /&gt;
        &lt;/And&gt;
      &lt;/Entry.Match&gt;
      &lt;Entry.SortBy&gt;
        &lt;Static /&gt;
        &lt;Access Order="public internal protected-internal protected private" /&gt;
        &lt;Name /&gt;
      &lt;/Entry.SortBy&gt;
    &lt;/Entry&gt;
    &lt;Entry DisplayName="Nested types"&gt;
      &lt;Entry.Match&gt;
        &lt;And&gt;
          &lt;Kind Is="type" /&gt;
        &lt;/And&gt;
      &lt;/Entry.Match&gt;
      &lt;Entry.SortBy&gt;
        &lt;Static /&gt;
        &lt;Access Order="public internal protected-internal protected private" /&gt;
        &lt;Name /&gt;
      &lt;/Entry.SortBy&gt;
    &lt;/Entry&gt;
  &lt;/TypePattern&gt;
&lt;/Patterns&gt;</s:String>

