	<!-- PERFORMANCE -->
	<!-- ─────────────────────────────────────────────────────────────────────────── -->
	<s:String x:Key="/Default/CodeInspection/Highlighting/InspectionSeverities/=ForeachCanBeConvertedToQueryUsingAnotherGetEnumerator/@EntryIndexedValue">HINT</s:String>
	<s:String x:Key="/Default/CodeInspection/Highlighting/InspectionSeverities/=LoopCanBeConvertedToQuery/@EntryIndexedValue">HINT</s:String>
	<s:String x:Key="/Default/CodeInspection/Highlighting/InspectionSeverities/=ForCanBeConvertedToForeach/@EntryIndexedValue">HINT</s:String>

	<!-- ─────────────────────────────────────────────────────────────────────────── -->
	<!-- RESHARPER SUPPRESSIONS - Intentional violations -->
	<!-- ─────────────────────────────────────────────────────────────────────────── -->
	<!-- AccessToDisposedClosure: Necessary in parallel operations with channels -->
	<s:String x:Key="/Default/CodeInspection/Highlighting/InspectionSeverities/=AccessToDisposedClosure/@EntryIndexedValue">HINT</s:String>

	<!-- InconsistentlySynchronizedField: Thread-safe operations use Interlocked/Volatile -->
	<s:String x:Key="/Default/CodeInspection/Highlighting/InspectionSeverities/=InconsistentlySynchronizedField/@EntryIndexedValue">HINT</s:String>

	<!-- InconsistentNaming: Allow for enum names like AIMD -->
	<s:String x:Key="/Default/CodeInspection/Highlighting/InspectionSeverities/=InconsistentNaming/@EntryIndexedValue">HINT</s:String>

