	<s:String x:Key="/Default/CodeStyle/CodeFormatting/CSharpCodeStyle/ContinuousIndentMultiplier/@EntryValue">1</s:String>

	<!-- ─────────────────────────────────────────────────────────────────────────── -->
	<!-- BRACES LAYOUT - Allman Style (Brace on New Line) -->
