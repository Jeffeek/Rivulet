	<!-- CODE INSPECTION SETTINGS -->
	<!-- ═══════════════════════════════════════════════════════════════════════════ -->

	<!-- ─────────────────────────────────────────────────────────────────────────── -->
	<!-- ASYNC/AWAIT - ConfigureAwait(false) enforcement -->
	<!-- ─────────────────────────────────────────────────────────────────────────── -->
	<s:String x:Key="/Default/CodeInspection/Highlighting/InspectionSeverities/=AwaitWithoutAwaitConfigureAwait/@EntryIndexedValue">WARNING</s:String>
	<s:String x:Key="/Default/CodeInspection/Highlighting/InspectionSeverities/=ConfigureAwaitChecker/@EntryIndexedValue">WARNING</s:String>

	<!-- ─────────────────────────────────────────────────────────────────────────── -->
	<!-- NULLABILITY - Enforce nullable reference types -->
	<!-- ─────────────────────────────────────────────────────────────────────────── -->
	<s:String x:Key="/Default/CodeInspection/Highlighting/InspectionSeverities/=PossibleNullReferenceException/@EntryIndexedValue">WARNING</s:String>
	<s:String x:Key="/Default/CodeInspection/Highlighting/InspectionSeverities/=AssignNullToNotNullAttribute/@EntryIndexedValue">WARNING</s:String>
	<s:String x:Key="/Default/CodeInspection/Highlighting/InspectionSeverities/=ConditionIsAlwaysTrueOrFalse/@EntryIndexedValue">WARNING</s:String>

	<!-- ─────────────────────────────────────────────────────────────────────────── -->
	<!-- DOCUMENTATION - XML comments required for public APIs -->
	<!-- ─────────────────────────────────────────────────────────────────────────── -->
	<s:String x:Key="/Default/CodeInspection/Highlighting/InspectionSeverities/=MissingXmlDoc/@EntryIndexedValue">WARNING</s:String>

